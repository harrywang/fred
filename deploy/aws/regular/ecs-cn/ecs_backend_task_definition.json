{
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "%s.dkr.ecr.cn-northwest-1.amazonaws.com.cn/fred-backend:prod",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "environment": [
        {
          "name": "APP_SETTINGS",
          "value": "app.config.ProductionConfig"
        },
        {
          "name": "DATABASE_TEST_URL",
          "value": "postgres://postgres:postgres@users-db:5432/users_test"
        },
        {
          "name": "DATABASE_URL",
          "value": "%s"
        },
        {
          "name": "SECRET_KEY",
          "value": "%s"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "cn-northwest-1"
        }
      }
    }
  ],
  "family": "fred-backend-td"
}
